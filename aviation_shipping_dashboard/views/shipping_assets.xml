<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <!-- Start - Common Dashboard  -->
        <record id="action_aviation_shipping_dashboard" model="ir.actions.client">
            <field name="name">Shipping</field>
            <field name="tag">aviation_shipping_dashboard</field>
        </record>

        <menuitem name="Shipping" id="menu_aviation_shipping_dashboard" action="action_aviation_shipping_dashboard"  web_icon="aviation_shipping_dashboard,static/description/icon.png"  sequence="1"/>
        <!-- End - Common Dashboard -->

        <template id="assets_backend_aviation_shipping_dashboard" name="Aviation Shipping Dashboard assets inherit" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                
                <script type="text/javascript" src="/aviation_shipping_dashboard/static/src/js/aviation_shipping_dashboard.js"/>
                <link rel="stylesheet" href="/aviation_shipping_dashboard/static/src/css/dashboard.css"/>
                              
            </xpath>
        </template>








                                                 
                                                 
    </data>
</odoo>